<script src="/assets/jquery.js" type="text/javascript"></script>
<script src="/assets/jquery_ujs.js" type="text/javascript"></script>

<%= link_to 'Submit', applicants_path, class: "btn btn-outline-secondary float-right" %>

<h1>New Application Please work</h1>

<div id="courses" class="list-group" data-url="<%= sort_courses_path %>">
  <% @courses.each do |course| %>
    <%= link_to course.name, course_path(course), class: "list-group-item", id: dom_id(course) %>
  <% end %>
</div>

